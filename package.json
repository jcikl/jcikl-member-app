{
  "name": "jci-kl-membership-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --skipLibCheck && vite build",
    "build:fast": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "firebase:login": "firebase login --reauth",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules",
    "firebase:deploy:indexes": "firebase deploy --only firestore:indexes",
    "firebase:deploy:all": "firebase deploy --only firestore",
    "seed:database": "vite-node src/scripts/seedDatabase.ts",
    "seed:settings": "vite-node src/scripts/seedGlobalSettings.ts",
    "backup:firestore": "vite-node src/scripts/backupFirestore.ts",
    "migrate:subcategory": "vite-node src/scripts/migrateSubCategoryToTxAccount.ts",
    "migrate:subcategory:dry": "vite-node src/scripts/migrateSubCategoryToTxAccount.ts -- --dry-run",
    "migrate:subcategory:rollback": "vite-node src/scripts/migrateSubCategoryToTxAccount.ts -- --rollback",
    "migrate:collections": "vite-node src/scripts/migrateCollectionNames.ts",
    "migrate:collections:dry": "vite-node src/scripts/migrateCollectionNames.ts -- --dry-run",
    "migrate:collections:rollback": "vite-node src/scripts/migrateCollectionNames.ts -- --rollback",
    "migrate:eventid": "vite-node src/scripts/migrateMetadataEventIdToRelatedEventId.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "antd": "^5.12.8",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "firebase": "^10.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^6.20.1",
    "yup": "^1.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "firebase-admin": "^13.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-node": "^3.2.4"
  }
}
